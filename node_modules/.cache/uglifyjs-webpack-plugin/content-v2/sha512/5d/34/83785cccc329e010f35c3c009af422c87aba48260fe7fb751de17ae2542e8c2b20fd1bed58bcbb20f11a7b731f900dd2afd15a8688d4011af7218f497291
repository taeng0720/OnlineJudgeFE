$success('Change email successfully')\\n            this.$refs.formEmail.resetFields()\\n          }, res => {\\n            if (res.data.data === 'tfa_required') {\\n              this.visible.tfaRequired = true\\n            }\\n          })\\n        })\\n      }\\n    }\\n  }\\n</script>\\n\\n<style lang=\\\"less\\\" scoped>\\n\\n  .flex-container {\\n    justify-content: flex-start;\\n    .left {\\n      flex: 1 0;\\n      width: 250px;\\n      padding-right: 5%;\\n    }\\n    > .middle {\\n      flex: none;\\n    }\\n    .right {\\n      flex: 1 0;\\n      width: 250px;\\n    }\\n  }\\n</style>\\n\\n\",\"var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',{staticClass:\\\"setting-main\\\"},[_c('div',{staticClass:\\\"flex-container\\\"},[_c('div',{staticClass:\\\"left\\\"},[_c('p',{staticClass:\\\"section-title\\\"},[_vm._v(_vm._s(_vm.$t('m.ChangePassword')))]),_vm._v(\\\" \\\"),_c('Form',{ref:\\\"formPassword\\\",staticClass:\\\"setting-content\\\",attrs:{\\\"model\\\":_vm.formPassword,\\\"rules\\\":_vm.rulePassword}},[_c('FormItem',{attrs:{\\\"label\\\":\\\"Old Password\\\",\\\"prop\\\":\\\"old_password\\\"}},[_c('Input',{attrs:{\\\"type\\\":\\\"password\\\"},model:{value:(_vm.formPassword.old_password),callback:function ($$v) {_vm.$set(_vm.formPassword, \\\"old_password\\\", $$v)},expression:\\\"formPassword.old_password\\\"}})],1),_vm._v(\\\" \\\"),_c('FormItem',{attrs:{\\\"label\\\":\\\"New Password\\\",\\\"prop\\\":\\\"new_password\\\"}},[_c('Input',{attrs:{\\\"type\\\":\\\"password\\\"},model:{value:(_vm.formPassword.new_password),callback:function ($$v) {_vm.$set(_vm.formPassword, \\\"new_password\\\", $$v)},expression:\\\"formPassword.new_password\\\"}})],1),_vm._v(\\\" \\\"),_c('FormItem',{attrs:{\\\"label\\\":\\\"Confirm New Password\\\",\\\"prop\\\":\\\"again_password\\\"}},[_c('Input',{attrs:{\\\"type\\\":\\\"password\\\"},model:{value:(_vm.formPassword.again_password),callback:function ($$v) {_v