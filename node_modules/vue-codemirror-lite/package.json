{
  "name": "vue-codemirror-lite",
  "version": "1.0.4",
  "description": "Lite Codemirror component for Vue.js",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node_modules/.bin/webpack-dev-server --open",
    "build:demo": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build": "webpack --config webpack.config.umd.js",
    "deploy": "cd demo-dist && git add --all && git commit -m \"Release at $(date)\" && git push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cnu4/vue-codemirror-lite.git"
  },
  "keywords": [
    "vue-codemirror-lite",
    "vue codemirror",
    "codemirror",
    "vue"
  ],
  "author": "fangxw",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cnu4/vue-codemirror-lite/issues"
  },
  "homepage": "https://github.com/cnu4/vue-codemirror-lite",
  "dependencies": {
    "codemirror": "^5.22.0"
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.26.0",
    "style-loader": "^0.13.1",
    "vue-loader": "^13.3.0",
    "vue-template-compiler": "^2.1.8",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^1.16.2"
  }
}
